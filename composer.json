{
    "name": "sonata-project/dashboard-bundle",
    "type": "symfony-bundle",
    "description": "Provides a Dashboard management through container and block services",
    "keywords": [
        "dashboard",
        "sonata"
    ],
    "homepage": "https://sonata-project.org/bundles/dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDashboardBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/orm": "^2.6",
        "sonata-project/admin-bundle": "^3.45",
        "sonata-project/block-bundle": "^3.18",
        "sonata-project/cache-bundle": "^2.1.7",
        "sonata-project/core-bundle": "^3.13.7",
        "sonata-project/datagrid-bundle": "^2.3",
        "sonata-project/doctrine-extensions": "^1.1.3",
        "sonata-project/easy-extends-bundle": "^2.3",
        "sonata-project/notification-bundle": "^3.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/options-resolver": "^3.4 || ^4.0",
        "symfony/security-core": "^3.4 || ^4.0",
        "twig/twig": "^1.41 || ^2.10"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\DashboardBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\DashboardBundle\\Tests\\": "tests/"
        }
    }
}
