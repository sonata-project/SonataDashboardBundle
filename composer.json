{
    "name": "sonata-project/dashboard-bundle",
    "type": "symfony-bundle",
    "description": "Provides a Dashboard management through container and block services",
    "keywords": [
        "dashboard",
        "sonata"
    ],
    "homepage": "https://sonata-project.org/bundles/dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDashboardBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/admin-bundle": "^3.4",
        "sonata-project/block-bundle": "^3.1.1",
        "sonata-project/cache-bundle": "^2.1.7",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/datagrid-bundle": "^2.2",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/easy-extends-bundle": "^2.2",
        "sonata-project/notification-bundle": "^3.0",
        "symfony/config": "^2.8 || ^3.2 || ^4.0",
        "symfony/dependency-injection": "^2.8 || ^3.2 || ^4.0",
        "symfony/http-kernel": "^2.8 || ^3.2 || ^4.0",
        "symfony/options-resolver": "^2.8 || ^3.2 || ^4.0",
        "symfony/security-core": "^2.8 || ^3.2 || ^4.0"
    },
    "require-dev": {
        "sonata-project/doctrine-orm-admin-bundle": "^3.0"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "^2.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\DashboardBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\DashboardBundle\\Tests\\": "tests/"
        }
    }
}
